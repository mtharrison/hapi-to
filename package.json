{
  "name": "hapi-to",
  "description": "Generate URIs based on named hapi routes, their ID and parameters",
  "keywords": [
    "hapi",
    "url",
    "uri",
    "route",
    "routes"
  ],
  "version": "1.0.0",
  "license": "BSD-3",
  "author": "Matt Harrison",
  "repository": {
    "type": "git",
    "url": "git@github.com:mtharrison/hapi-to.git"
  },
  "bugs": {
    "url": "https://github.com/mtharrison/hapi-to/issues"
  },
  "homepage": "https://github.com/mtharrison/hapi-to",
  "main": "./lib/index.js",
  "config": {
    "tapeOptions": "-r babel-core/register test/**/*.spec.js"
  },
  "scripts": {
    "build": "babel -d lib src",
    "coverage": "NODE_ENV=test istanbul cover tape -- $npm_package_config_tapeOptions | tap-spec",
    "lint": "eslint --ext .js src",
    "start": "watch 'npm test' src test --wait 5 --interval 1",
    "test": "NODE_ENV=test tape $npm_package_config_tapeOptions | tap-spec",
    "prebuild": "rm -rf lib",
    "precoverage": "rm -rf coverage",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "2.11.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.2.3",
    "hapi": "^13.4.1",
    "istanbul": "^1.0.0-alpha.2",
    "sinon": "^1.17.4",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "watch": "^0.18.0"
  },
  "dependencies": {
    "hoek": "^4.0.1",
    "joi": "^8.4.0",
    "qs": "^6.2.0"
  },
  "peerDependencies": {
    "hapi": "13.x.x"
  }
}
